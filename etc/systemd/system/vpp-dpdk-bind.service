[Unit]
Description=FloofOS DPDK Interface Binding
DefaultDependencies=no
Before=network-pre.target systemd-networkd.service NetworkManager.service
After=systemd-modules-load.service
Wants=systemd-modules-load.service

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/sbin/vpp-dpdk-bind

[Install]
WantedBy=sysinit.target

